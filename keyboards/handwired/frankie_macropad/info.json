{
    "keyboard_name": "frankie_macropad",
    "manufacturer": "Eric Wood",
    "url": "https://github.com/eric-wood/macro-pad",
    "maintainer": "eric-wood",
    "usb": {
        "vid": "0xF00D",
        "pid": "0x0000",
        "device_version": "0.0.1"
    },
    "matrix_pins": {
        "cols": ["B3", "B4", "B5", "B6"],
        "rows": ["B0", "B1", "B2"]
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            {"pin_a": "D0", "pin_b": "D1"},
            {"pin_a": "D2", "pin_b": "D3"}
        ]
    },
    "bootmagic": {
        "matrix": [2, 0]
    },
    "processor": "atmega16u2",
    "bootloader": "atmel-dfu",
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"label": "k02", "x": 2, "y": 0},
                {"label": "k03", "x": 3, "y": 0},

                {"label": "k10", "x": 0, "y": 1},
                {"label": "k11", "x": 1, "y": 1},
                {"label": "k12", "x": 2, "y": 1},
                {"label": "k13", "x": 3, "y": 1},

                {"label": "k20", "x": 0, "y": 2},
                {"label": "k21", "x": 1, "y": 2},
                {"label": "k22", "x": 2, "y": 2},
                {"label": "k23", "x": 3, "y": 2}
            ]
        }
    }
}
