{
    "keyboard_name": "Tenkey++",
    "manufacturer": "Lizard Trick Electronics",
    "url": "https://github.com/jonbobcar/tenkey_plusplus",
    "maintainer": "Jonathon Carstens",
    "usb": {
        "vid": "0xFEED",
        "pid": "0x0000",
        "device_version": "0.0.1"
    },
    "matrix_pins": {
        "cols": ["D5", "D3", "D2", "F7"],
        "rows": ["B7", "D4", "B5", "B6", "C6", "C7"]
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            {"pin_a": "B4", "pin_b": "D7"},
            {"pin_a": "F0", "pin_b": "E6"},
            {"pin_a": "F4", "pin_b": "F1"}
        ]
    },
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu",
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"label": "Mute", "x": 0, "y": 0, "w": 1.3},
                {"label": "MDPL", "x": 1.3, "y": 0, "w": 1.4},
                {"label": "MDNX", "x": 2.7, "y": 0, "w": 1.3},

                {"label": "Num Lock", "x": 0, "y": 1},
                {"label": "/", "x": 1, "y": 1},
                {"label": "*", "x": 2, "y": 1},
                {"label": "-", "x": 3, "y": 1},

                {"label": "7", "x": 0, "y": 2},
                {"label": "8", "x": 1, "y": 2},
                {"label": "9", "x": 2, "y": 2},
                {"label": "+", "x": 3, "y": 2, "h": 2},

                {"label": "4", "x": 0, "y": 3},
                {"label": "5", "x": 1, "y": 3},
                {"label": "6", "x": 2, "y": 3},

                {"label": "1", "x": 0, "y": 4},
                {"label": "2", "x": 1, "y": 4},
                {"label": "3", "x": 2, "y": 4},
                {"label": "Enter", "x": 3, "y": 4, "h" :2},

                {"label": "0", "x": 0, "y": 5, "w": 2},
                {"label": ".", "x": 2, "y": 5}
            ]
        }
    }
}
